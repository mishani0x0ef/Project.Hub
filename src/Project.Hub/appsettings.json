{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning"
    }
  },
  "AppConfiguration": {
    "AppDisplayName": "Project Hub",
    "ConfigPath": "hub.config.json",
    "VersionsCacheExpirationInHours": 8,
    // https://stackoverflow.com/questions/7908343/list-of-timezone-ids-for-use-with-findtimezonebyid-in-c?utm_medium=organic&utm_source=google_rich_qa&utm_campaign=google_rich_qa
    "DisplayTimeZone": "FLE Standard Time",
    // Environment or absolute path to powershell (required v5.1 or higher).
    // 'powershell' for default PowerShell
    // 'Set-Location "${env:ProgramFiles}\\PowerShell\\6-preview"; .\\pwsh.exe' for PowerShell Core
    "PowerShellPath": "powershell"
  }
}
