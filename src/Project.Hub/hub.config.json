{
  "Version": "2.0",

  "CommonServices": [
    {
      "Name": "GitHub",
      "Description": "Link to the project on the GitHub.",
      "Url": "https://github.com/mishani0x0ef/Project.Hub"
    },
    {
      "Name": "Google Analytics",
      "Description": "Link to the project's analytics.",
      "Url": "https://analytics.google.com",
      "FaviconFallback": "https://cdn0.iconfinder.com/data/icons/icocentre-free-icons/171/f-analytics2_128-512.png"
    }
  ],

  "Environments": [
    {
      "Name": "Testing",
      "Description": "Testing environment."
    },
    {
      "Name": "Production",
      "Description": "Production environment."
    }
  ],

  "Websites": [
    {
      "Name": "GitHub",
      "Description": "Could be a link to any website. Click causing redirect.",
      "Tags": [ "useful", "github-team" ],
      "Environments": [
        {
          "Environment": "Testing",
          "Url": "https://github.com"
        },
        {
          "Environment": "Production",
          "Url": "https://github.com"
        }
      ]
    }
  ],

  "Apis": [
    {
      "Name": "GitHub API",
      "Description": "Could be link to any API (I suggest you Swagger-link). Click causing redirect.",
      "Tags": [ "security", "github-team" ],
      "Environments": [
        {
          "Environment": "Testing",
          "Url": "https://docs.github.com/en/graphql"
        },
        {
          "Environment": "Production",
          "Url": "https://docs.github.com/en/graphql"
        }
      ]
    }
  ],

  "Downloads": [
    {
      "Name": "Visual Studio",
      "Description": "Download file using web-link to this file.",
      "Tags": [ "desktop" ],
      "Environments": [
        {
          "Environment": "Testing",
          "DownloadPath": "https://visualstudio.microsoft.com/thank-you-downloading-visual-studio/?sku=Community&rel=16"
        },
        {
          "Environment": "Production",
          "DownloadPath": "https://visualstudio.microsoft.com/thank-you-downloading-visual-studio/?sku=Community&rel=16"
        }
      ]
    },
    {
      "Name": "Hub Configuration",
      "Description": "Download file from file system.",
      "Mode": "FileSystem",
      "Tags": [ "file-system", "desktop", "github-team" ],
      "Environments": [
        {
          "Environment": "Testing",
          "DownloadPath": ".\\hub.config.json",
          "VersionOptions": {
            "Type": "Assembly",
            "Path": ".\\Project.Hub.dll"
          }
        },
        {
          "Environment": "Production",
          "DownloadPath": ".\\hub.config.json",
          "VersionOptions": {
            "Type": "Assembly",
            "Path": ".\\Project.Hub.Config.dll"
          }
        }
      ]
    }
  ]
}
